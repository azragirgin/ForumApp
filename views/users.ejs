<!-- users.ejs -->
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>List of Users</title>
    <link rel="stylesheet" type="text/css" href="/main.css" />
  </head>
  <body>
    <h1>List of Users</h1>
    <ul>
      <% for (let i = 0; i < users.length; i++) { %>
      <li><%= users[i].username %> - <%= users[i].email %></li>
      <% } %>
    </ul>
  </body>
</html>
